<?xml version="1.0" encoding="UTF-8"?>
<Translation>

	<!-- Favorites -->

	<AddToFavoritesWindow Caption='Add a Favorite'>
		<Label1 Caption='Name:'/>
		<Label2 Caption='Create in:'/>

		<btnAdd Caption='Add'/>
		<btnCancel Caption='Cancel'/>
		<btnNewFolder Caption='New folder'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='Favorites'/>
				<Item Index='1' Value='Create a Folder'/>
				<Item Index='2' Value='Folder name: '/>
			</StringList>
		</AeroWindow>
	</AddToFavoritesWindow>

	<OrganizeFavoritesWindow Caption='Organize Favorites - $AppName$'>
		<btnSave Caption='Save'/>
		<btnCancel Caption='Cancel'/>
		<btnNewFolder Caption='New folder'/>
		<btnRemove Caption='Remove'/>
		<btnClear Caption='Clear'/>

		<Label1 Caption='Use drag and drop to organize your favorites.'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='Favorites'/>
				<Item Index='1' Value='Create a Folder'/>
				<Item Index='2' Value='Folder name: '/>
			</StringList>
		</AeroWindow>
	</OrganizeFavoritesWindow>

	<!-- Archive Defragmenter -->

	<DefragmenterWindow Caption='Archive Defragmenter - $AppName$'>
		<!-- Labels -->
		<lbDescription Caption="Archive defragmenter consolidates files inside archive to reduce archive size on your computer's hard disk."/>
		<lbNoteSave Caption='All your changes are saved already, you do not need to use defragmentation after each editing.'/>
		<lbNoteRevert Caption='The defragmentation feature will not revert any changes you made in archives.'/>
		<!-- Actions -->
		<actClose Caption='Close'/>
		<actRebuild Caption='Defragment'/>
		<actAddFile Caption='Add files'/>
		<actAddFolder Caption='Add folder'/>
		<actRemoveFile Caption='Remove file'/>
		<actClear Caption='Clear list'/>
		<!-- Columns -->
		<lvFiles>
			<Columns>
				<Item Index='0' Caption=''/>
				<Item Index='1' Caption='Name'/>
				<Item Index='2' Caption='Location'/>
				<Item Index='3' Caption='Type'/>
			</Columns>
		</lvFiles>
		<!-- Messages -->
		<AeroWindow LinesCount='4'>
			<StringList>
				<Item Index='0' Value='All complete.'/>
				<Item Index='1' Value='Archive Defragmenter - $AppName$'/>
				<Item Index='2' Value='Open rebuilt archive?'/>
				<Item Index='3' Value='Some files are not processed.'/>
			</StringList>
		</AeroWindow>
	</DefragmenterWindow>

	<!-- Archive properties -->

	<ArchivePropertiesWindow Caption='Archive information'>
		<!-- Button's -->
		<btnClose Caption='Close'/>
		<!-- Label's -->
		<lbType Caption='Type:'/>
		<lbVersion Caption='Version:'/>
		<lbCount Caption='Items count:'/>

		<lbCGame Caption='Current game:'/>
		<lbCPlatform Caption='Current platform:'/>

		<lbCodeModule Caption='Module:'/>
		<lbCMVersion Caption='Module version:'/>
	</ArchivePropertiesWindow>

	<!-- openFormats -->

	<openFormatsImportWindow Caption='Import openFormats - $AppName$'>
		<!-- Label's -->
		<lbOfFileName Caption='Import file:'/>
		<lbOfFiles Caption='Import files:'/>
		<lbNewFileName Caption='New file name:'/>
		<!-- Button's -->
		<btnAdd Caption='Add'/>
		<btnRemove Caption='Remove'/>
		<btnCancel Caption='Cancel'/>
		<btnOk Caption='Ok'/>
		<tbLeanMore Caption='Learn more about openFormats'/>
		<!-- Check box -->
		<cbProtectMesh Caption='Protect mesh (prevent future export)'/>
	</openFormatsImportWindow>

	<openFormatsValidatorWindow Caption='openFormats validator - $AppName$'>
		<btnClose Caption='Close'/>
		<!-- Strings -->
		<AeroWindow LinesCount='100'>
			<StringList>
				<Item Index='0' Value='Some files not imported, more information below.'/>
				<!-- Version errors  -->
				<Item Index='1' Value='%S(Line %D): Not supported resource version, actual: "%S" but expected "%D".'/>
				<Item Index='2' Value='%S(Line %D): Not supported format version, actual: "%S" but expected "%D".'/>
				<!-- Common errors -->
				<Item Index='3' Value='Fatal error: Unable to initialize Direct3D.'/>
				<Item Index='4' Value='Fatal error: File "%S" not found.'/>
				<Item Index='5' Value='%S(Line %D): File "%S" not found.'/>
				<Item Index='6' Value='%S(Line %D): File corrupted.'/>
				<Item Index='7' Value='%S(Line %D): Property "%s" expected but "%s" found.'/>
				<Item Index='8' Value='%S(Line %D): Property "%s" not found.'/>
				<Item Index='9' Value='%S(Line %D): Not enough actual parameters.'/>
				<!-- Collision errors -->
				<Item Index='10' Value='%S(Line %D): Unknown collision bound type "%S".'/>
				<Item Index='11' Value='%S(Line %D): Collision too big.'/>
				<Item Index='12' Value='%S(Line %D): Collision corrupted.'/>
				<!-- Skeleton errors -->
				<Item Index='13' Value='%S(Line %D): Bone name is null.'/>
				<!-- Geometry/Graphics/Drawables errors -->
				<Item Index='14' Value='%S(Line %D): Mesh is locked.'/>
				<Item Index='15' Value='%S(Line %D): Material index out of bounds.'/>
				<Item Index='16' Value="%S(Line %D): Shader can't be skinned."/>
				<Item Index='17' Value="%S(Line %D): Too many bones in one geometry, you can't use more than 48 bones."/>

				<Item Index='18' Value="%S(Line %D): %S"/>

				<!-- Export/Import -->
				<Item Index='19' Value='openFormats import report'/>
				<Item Index='20' Value='openFormats export report'/>
				<Item Index='21' Value='Some files not exported, more information below.'/>

			</StringList>
		</AeroWindow>
	</openFormatsValidatorWindow>

</Translation>
