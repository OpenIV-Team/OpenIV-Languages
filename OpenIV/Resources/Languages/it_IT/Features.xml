<?xml version="1.0" encoding="UTF-8"?>
<Translation>

	<!-- Favorites -->

	<AddToFavoritesWindow Caption='Aggiungi Favorito'>
		<Label1 Caption='Nome:'/>
		<Label2 Caption='Crea in:'/>

		<btnAdd Caption='Aggiungi'/>
		<btnCancel Caption='Elimina'/>
		<btnNewFolder Caption='Nuova Cartella'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='Favoriti'/>
				<Item Index='1' Value='Crea una nuova cartella'/>
				<Item Index='2' Value='Nome Cartella: '/>
			</StringList>
		</AeroWindow>
	</AddToFavoritesWindow>

	<OrganizeFavoritesWindow Caption='Organizza Favoriti - $AppName$'>
		<btnSave Caption='Salva'/>
		<btnCancel Caption='Elimina'/>
		<btnNewFolder Caption='Nuova Cartella'/>
		<btnRename Caption='Rinomina'/>
		<btnRemove Caption='Rimuovi'/>
		<btnClear Caption='Pulisci'/>

		<Label1 Caption='Usa il trascinamento per organizzare i tuoi preferiti.'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='Favoriti'/>
				<Item Index='1' Value='Crea Cartella'/>
				<Item Index='2' Value='Nome Cartella: '/>
			</StringList>
		</AeroWindow>
	</OrganizeFavoritesWindow>

	<!-- Archive Defragmenter -->

	<DefragmenterWindow Caption='Deframmentazione Archivi - $AppName$'>
		<!-- Labels -->
		<lbDescription Caption="L'utilità di deframmentazione archivi sposta i file all'interno dell'archivio per ridurre la quantità di spazio occupato dall'archivio sul disco rigido."/>
		<lbNoteSave Caption='Tutte le modifiche sono già state salvate, non è necessario usare la deframmentazione dopo ogni modifica.'/>
		<lbNoteRevert Caption="La deframmentazione dell'archivio non annullerà le modifiche apportate e non restituirà i file originali"/>
		<!-- Actions -->
		<actClose Caption='Chiudi'/>
		<actRebuild Caption='Deframmenta'/>
		<actAddFile Caption='Aggiungi file'/>
		<actAddFolder Caption='Aggiungi cartella'/>
		<actRemoveFile Caption='Rimuovi file'/>
		<actClear Caption='Pulisci lista'/>
		<!-- Columns -->
		<lvFiles>
			<Columns>
				<Item Index='0' Caption=''/>
				<Item Index='1' Caption='Nome'/>
				<Item Index='2' Caption='Posizione'/>
				<Item Index='3' Caption='Tipo'/>
			</Columns>
		</lvFiles>
		<!-- Messages -->
		<AeroWindow LinesCount='4'>
			<StringList>
				<Item Index='0' Value='Completato.'/>
				<Item Index='1' Value='Deframmentazione Archivi - $AppName$'/>
				<Item Index='2' Value="Aprire l'archivio ricostruito?"/>
				<Item Index='3' Value='Alcuni file non vengono elaborati'/>
			</StringList>
		</AeroWindow>
	</DefragmenterWindow>

	<!-- Archive properties -->

	<ArchivePropertiesWindow Caption="Informazioni Archivio">
		<!-- Button's -->
		<btnClose Caption='Chiudi'/>
		<!-- Label's -->
		<lbType Caption='Tipo:'/>
		<lbVersion Caption='Versione:'/>
		<lbCount Caption='Numero di Oggetti:'/>

		<lbCGame Caption='Gioco:'/>
		<lbCPlatform Caption='Piattaforma:'/>

		<lbCodeModule Caption='Modulo:'/>
		<lbCMVersion Caption='Versione Modulo:'/>
	</ArchivePropertiesWindow>

	<!-- openFormats -->

	<openFormatsImportWindow Caption='Importa openFormats - $AppName$'>
		<!-- Label's -->
		<lbOfFileName Caption='File importato:'/>
		<lbOfFiles Caption='Importa file:'/>
		<lbNewFileName Caption='Nuovo nome file:'/>
		<!-- Button's -->
		<btnAdd Caption='Aggiungi'/>
		<btnRemove Caption='Rimuovi'/>
		<btnCancel Caption='Elimina'/>
		<btnOk Caption='Ok'/>
		<tbLeanMore Caption='Impara di più su openFormats'/>
		<!-- Check box -->
		<cbProtectMesh Caption='Proteggi geometria (Impedisce future exportazioni)'/>
	</openFormatsImportWindow>

	<openFormatsValidatorWindow Caption='openFormats Validatore - $AppName$'>
		<btnClose Caption='Chiudi'/>
		<!-- Strings -->
		<AeroWindow LinesCount='100'>
			<StringList>
				<Item Index='0' Value='Alcuni file non vengono importati, più informazioni sono disponibili sotto.'/>
				<!-- Version errors  -->
				<Item Index='1' Value='%S(Line %D): Versione risorsa non supportata, attuale: "%S" ma attesa "%D".'/>
				<Item Index='2' Value='%S(Line %D): Versione formato non supportato, attuale: "%S" ma attesa "%D".'/>
				<!-- Common errors -->
				<Item Index='3' Value='Errore Fatale: Impossibile inizializzare Direct3D.'/>
				<Item Index='4' Value='Errore Fatale: File "%S" non trovato.'/>
				<Item Index='5' Value='%S(Linea %D): File "%S" non trovato.'/>
				<Item Index='6' Value='%S(Linea %D): File corrotto.'/>
				<Item Index='7' Value='%S(Linea %D): Proietà "%s" aspettata ma "%s" trovata.'/>
				<Item Index='8' Value='%S(Linea %D): Proprietà "%s" non trovata.'/>
				<Item Index='9' Value='%S(Linea %D): Parametri attuali insufficienti.'/>
				<!-- Collision errors -->
				<Item Index='10' Value='%S(Linea %D): Limite di collisione sconosciuto  "%S".'/>
				<Item Index='11' Value='%S(Linea %D): Collisione troppo grande.'/>
				<Item Index='12' Value='%S(Linea %D): Collisione corrotta.'/>
				<!-- Skeleton errors -->
				<Item Index='13' Value='%S(Linea %D): Il nome dello scheletro è nullo.'/>
				<!-- Geometry/Graphics/Drawables errors -->
				<Item Index='14' Value='%S(Linea %D): La mesh e bloccata.'/>
				<Item Index='15' Value='%S(Linea %D): Indice materiale fuori limite.'/>
				<Item Index='16' Value="%S(Linea %D): L'ombreggiatura non può essere spellata."/>
				<Item Index='17' Value="%S(Linea %D): Troppe ossa in una sola geometria, non puoi usarne più di 48."/>

				<Item Index='18' Value="%S(Linea %D): %S"/>

				<!-- Export/Import -->
				<Item Index='19' Value="rapporto dell'importazione openFormats "/>
				<Item Index='20' Value="rapporto dell'esportazione openFormats"/>
				<Item Index='21' Value='Alcuni file non sono stanti esportati, ulteriori informazioni di seguito.'/>

			</StringList>
		</AeroWindow>
	</openFormatsValidatorWindow>

</Translation>
