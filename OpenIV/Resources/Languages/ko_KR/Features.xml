		<btnAdd Caption='추가'/>
		<btnCancel Caption='취소'/>
		<btnNewFolder Caption='새 폴더'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='즐겨찾기'/>
				<Item Index='1' Value='새 폴더 만들기'/>
				<Item Index='2' Value='폴더 이름: '/>
			</StringList>
		</AeroWindow>
	</AddToFavoritesWindow>

	<OrganizeFavoritesWindow Caption='$AppName$ - 즐겨찾기 구성'>
		<btnSave Caption='저장'/>
		<btnCancel Caption='취소'/>
		<btnNewFolder Caption='새 폴더'/>
		<btnRemove Caption='삭제'/>
		<btnClear Caption='지우기'/>

		<Label1 Caption='드래그 앤 드롭을 사용하여 즐겨찾기를 구성합니다.'/>

