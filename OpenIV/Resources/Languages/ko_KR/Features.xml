		<lbCodeModule Caption='모듈:'/>
		<lbCMVersion Caption='모듈 버전:'/>
	</ArchivePropertiesWindow>

	<!-- openFormats -->

	<openFormatsImportWindow Caption='$AppName$ - openFormats 불러오기'>
		<!-- Label's -->
		<lbOfFileName Caption='파일 불러오기:'/>
		<lbOfFiles Caption='파일 불러오기:'/>
		<lbNewFileName Caption='새 파일 이름:'/>
		<!-- Button's -->
		<btnAdd Caption='추가'/>
		<btnRemove Caption='삭제'/>
		<btnCancel Caption='취소'/>
		<btnOk Caption='확인'/>
		<tbLeanMore Caption='openFormats 파일에 대해 자세히 알아보기'/>
		<!-- Check box -->
		<cbProtectMesh Caption='메쉬 보호 (prevent future export)'/>
	</openFormatsImportWindow>

	<openFormatsValidatorWindow Caption='$AppName$ - openFormats 유효성 검사기'>
		<btnClose Caption='취소'/>
		<!-- Strings -->
		<AeroWindow LinesCount='100'>
			<StringList>
				<Item Index='0' Value='일부 파일을 가져 오지 못했습니다. 자세한 내용은 아래에서 확인하세요.'/>
				<!-- Version errors  -->
				<Item Index='1' Value='%S(%D 줄): 리소스 버전이 지원되지 않습니다. 실제: "%S"이지만 "%D"가 필요합니다.'/>
				<Item Index='2' Value='%S(%D 줄): 지원되지 않는 형식 버전입니다. 실제: "%S"이지만 "%D"가 필요합니다.'/>
				<!-- Common errors -->
				<Item Index='3' Value='치명적 오류: Direct3D를 초기화 할 수 없습니다.'/>
				<Item Index='4' Value='치명적 오류: "%S" 파일을 찾을 수 없습니다.'/>
				<Item Index='5' Value='%S(%D 줄): "%S" 파일을 찾을 수 없습니다.'/>
				<Item Index='6' Value='%S(%D 줄): 파일이 손상되었습니다.'/>
				<Item Index='7' Value='%S(%D 줄): "%s" 속성이 필요하지만 "%s"이(가) 발견되었습니다.'/>
				<Item Index='8' Value='%S(%D 줄): "%s" 속성을 찾을 수 없습니다.'/>
				<Item Index='9' Value='%S(%D 줄): 실제 매개 변수가 부족합니다.'/>
				<!-- Collision errors -->
				<Item Index='10' Value='%S(%D 줄): "%S" 알 수 없는 충돌 바인딩 유형입니다.'/>
				<Item Index='11' Value='%S(%D 줄): 충돌이 너무 잦습니다.'/>
				<Item Index='12' Value='%S(%D 줄): 충돌 오류를 일으켰습니다.'/>
				<!-- Skeleton errors -->
				<Item Index='13' Value='%S(%D 줄): 골격 이름은 비울 수 없습니다.'/>
				<!-- Geometry/Graphics/Drawables errors -->
				<Item Index='14' Value='%S(%D 줄): 메시가 잠겨 있습니다.'/>
				<Item Index='15' Value='%S(%D 줄): 마테리얼 인덱스가 범위를 벗어났습니다'/>
				<Item Index='16' Value="%S(%D 줄): 셰이더는 벗길 수 없습니다."/>
				<Item Index='17' Value="%S(%D 줄): Too many bones in one geometry, you can't use more than 48 bones."/>

				<Item Index='18' Value="%S(%D 줄): %S"/>

				<!-- Export/Import -->
				<Item Index='19' Value='openFormats import report'/>
				<Item Index='20' Value='openFormats export report'/>
				<Item Index='21' Value='일부 파일을 내보내지 않았습니다. 자세한 내용은 아래를 참조하십시오.'/>

			</StringList>
		</AeroWindow>
	</openFormatsValidatorWindow>

